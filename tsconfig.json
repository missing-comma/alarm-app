{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "es2015",
      "es2016"
    ],
    "jsx": "preserve",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "__tests__"
  ],
  "extends": "expo/tsconfig.base"
}
